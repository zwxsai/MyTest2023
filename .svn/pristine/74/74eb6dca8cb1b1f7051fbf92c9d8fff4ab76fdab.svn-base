<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- 访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!-- SD卡读取权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- 允许程序设置内置sd卡的写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".base.MyApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <!--
            android:exported    是否支持其它应用调用当前组件
            默认值：如果主页包含有intent-filter 默认值为true; 没有intent-filter默认值为false
        -->
        <activity
            android:name=".module.Home.SplashActivity"
            android:exported="true"
            android:theme="@style/SplashStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module.Home.GuideActivity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.FoundationActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.Open2Activity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.ServiceActivity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.ContentProviderActivity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.BroadcastReceiverActivity"
            android:exported="false" />
        <activity
            android:name=".module.Home.MainActivity"
            android:exported="false"
            android:launchMode="singleTask" />
        <activity
            android:name=".module.Foundation.DesignActivity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.SuanFaActivity"
            android:exported="false" />
        <activity
            android:name=".module.Foundation.ShuJuJieGouActivity"
            android:exported="false" />

        <service android:name=".module.Foundation.FoundationService1" />
        <service android:name=".module.Foundation.FoundationService2" /> <!-- 静态注册 -->
        <receiver
            android:name=".module.Foundation.MyReceiver"
            android:enabled="true"
            android:exported="false">
            <intent-filter android:priority="1000">
                <action android:name="MyReceiver" />
            </intent-filter>
        </receiver>


        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5b223d07a40fa3543800009e" /> <!-- 友盟 Appkey 自己应用注册申请来的 -->
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" /> <!-- 渠道号，多渠道这里使用了占位符$ -->

    </application>

</manifest>